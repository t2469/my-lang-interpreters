関数 fizzbuzz(n) {
    i := 1;
    繰り返し n {
        もし i % 15 == 0 ならば {
            出力 "FizzBuzz";
        } そうでないなら もし i % 3 == 0 ならば {
            出力 "Fizz";
        } そうでないなら もし i % 5 == 0 ならば {
            出力 "Buzz";
        } そうでないなら {
            出力 i;
        }
        i := i + 1;
    }
}

fizzbuzz(20);